{
  "name": "@argonjs/argon",
  "version": "1.4.0-51",
  "main": "dist/argon.core.js",
  "browser": "dist/argon.js",
  "typings": "index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/argonjs/argon"
  },
  "jspm": {
    "format": "esm",
    "jspmPackage": true,
    "main": "src/argon.js",
    "dependencies": {
      "cesium": "github:aelatgt/cesium@referenceFrames",
      "aurelia-dependency-injection": "npm:aurelia-dependency-injection@^1.2.0",
      "aurelia-polyfills": "npm:aurelia-polyfills@^1.0.0-beta.1.0.0",
      "googlevr/webvr-polyfill": "github:googlevr/webvr-polyfill@0.9.27",
      "json": "github:systemjs/plugin-json@^0.2.3",
      "object-assign": "npm:object-assign@^4.1.0",
      "orientationchangeend": "npm:orientationchangeend@^1.1.2"
    },
    "devDependencies": {
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "chai": "npm:chai@^3.5.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "css": "github:systemjs/plugin-css@^0.1.19",
      "diff": "npm:diff@^3.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "mocha": "npm:mocha@^3.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "ts": "github:frankwallis/plugin-typescript@^5.3.3",
      "typescript": "npm:typescript@^2.2.1",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "aurelia-metadata": "npm:aurelia-metadata@^1.0.0",
      "aurelia-pal": "npm:aurelia-pal@^1.0.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0"
    },
    "overrides": {
      "npm:aurelia-dependency-injection@1.2.0": {
        "main": "dist/native-modules/aurelia-dependency-injection.js",
        "directories": null,
        "format": "esm"
      },
      "npm:aurelia-metadata@1.0.2": {
        "main": "dist/native-modules/aurelia-metadata.js",
        "directories": null,
        "format": "esm"
      },
      "npm:aurelia-pal@1.0.0": {
        "main": "dist/native-modules/aurelia-pal.js",
        "directories": null,
        "format": "esm"
      },
      "npm:chai@3.5.0": {
        "main": "index.js",
        "format": "cjs",
        "jspmPackage": true,
        "meta": {
          "*": {
            "globals": {
              "process": "process"
            }
          },
          "*.json": {
            "format": "json"
          },
          "chai.js": {
            "cjsRequireDetection": false
          }
        },
        "map": {
          "./lib/chai/utils": "./lib/chai/utils/index.js",
          "process": "@empty"
        }
      },
      "npm:debug@2.6.0": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "fs": "@empty",
          "net": "@empty",
          "tty": "@empty",
          "util": "@empty"
        }
      },
      "npm:deep-eql@0.1.3": {
        "main": "index.js",
        "format": "cjs",
        "meta": {
          "*.json": {
            "format": "json"
          }
        },
        "map": {
          "buffer": "@empty"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "github:aelatgt/cesium@referenceFrames": {
        "registry": "github",
        "dependencies": {},
        "map": {
          "./Source/DataSources/BillboardGraphics": "@empty",
          "./Source/DataSources/BoxGraphics": "@empty",
          "./Source/DataSources/CorridorGraphics": "@empty",
          "./Source/DataSources/CylinderGraphics": "@empty",
          "./Source/DataSources/EllipseGraphics": "@empty",
          "./Source/DataSources/EllipsoidGraphics": "@empty",
          "./Source/DataSources/LabelGraphics": "@empty",
          "./Source/DataSources/ModelGraphics": "@empty",
          "./Source/DataSources/PathGraphics": "@empty",
          "./Source/DataSources/PointGraphics": "@empty",
          "./Source/DataSources/PolygonGraphics": "@empty",
          "./Source/DataSources/PolylineGraphics": "@empty",
          "./Source/DataSources/PolylineVolumeGraphics": "@empty",
          "./Source/DataSources/RectangleGraphics": "@empty",
          "./Source/DataSources/WallGraphics": "@empty"
        }
      },
      "npm:mocha@3.2.0": {
        "main": "./mocha.js",
        "jspmNodeConversion": false,
        "meta": {
          "mocha.js": {
            "deps": [
              "./mocha.css!"
            ],
            "exports": "Mocha",
            "format": "global"
          }
        },
        "dependencies": {
          "css": "*",
          "debug": "^2.2.0",
          "json3": "npm:json3@3.3.2",
          "lodash.create": "npm:lodash.create@3.1.1"
        },
        "registry": "jspm",
        "map": {
          "./mocha.js": {
            "node": "./lib/mocha.js"
          },
          "escape-string-regexp": "@empty",
          "growl": "@empty",
          "./lib/reporters.js": "./lib/reporters/index.js",
          "./lib/interfaces.js": "./lib/interfaces/index.js",
          "./lib/to-iso-string.js": "./lib/to-iso-string/index.js",
          "glob": "@empty",
          "mkdirp": "@empty",
          "jade": "@empty",
          "supports-color": "@empty",
          "path": "@node/path",
          "fs": "@node/fs",
          "util": "@node/util",
          "events": "@node/events",
          "tty": "@node/tty"
        }
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:typescript@2.1.4": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      },
      "npm:typescript@2.2.1": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        },
        "dependencies": {
          "source-map-support": "*"
        }
      }
    }
  },
  "scripts": {
    "format": "tsfmt -r",
    "clean": "rm -rf .tmp",
    "bundle-deps": "test -f .tmp/deps-bundle.js || jspm bundle 'src/**/* - [src/**/*]' .tmp/deps-bundle.js --inject",
    "build:browser": "jspm build src/argon.ts .tmp/argon-build.js --format umd --global-name Argon --skip-source-maps && derequire .tmp/argon-build.js > dist/argon.js",
    "build:core": "jspm build src/argon.ts .tmp/argon-build.js --node --format umd --global-name Argon --skip-source-maps && derequire .tmp/argon-build.js > dist/argon.core.js",
    "build": "npm run bundle-deps && npm run build:browser",
    "uglify": "uglifyjs dist/argon.js -m -c -o dist/argon.min.js",
    "build-full": "npm run clean && npm run bundle-deps && npm run build:core && npm run build:browser && npm run uglify",
    "browser-sync": "npm run bundle-deps && browser-sync  start --server --directory --startPath \"index.html\" --files \"src/**\"",
    "dev": "npm run bundle-deps && http-server -p 3000",
    "test": "jspm run test/runner.js",
    "test-debug": "node --inspect --debug-brk node_modules/.bin/jspm run test/runner.js",
    "static": "static -p 3000",
    "typedoc": "typedoc --tsconfig tsconfig.build.json --out docs --mode file --gaID UA-63191442-2 --name argon.js --readme src/README.md --target ES6 --module commonjs --moduleResolution node --experimentalDecorators --includeDeclarations --excludeNotExported --excludeExternals --ignoreCompilerErrors src",
    "postinstall": "npm install types/cesium",
    "tsc": "tsc",
    "preversion": "[[ -z $(git status --porcelain) ]]",
    "version": "npm run tsc && npm run build-full && git add -u",
    "postversion": "git push && git push --tags",
    "check-git-status": "[[ -z $(git status --porcelain) ]] && npm run tsc",
    "release:next": "standard-version --commit-all --prerelease --skip.changelog true && npm publish --tag next",
    "release:latest": "standard-version --commit-all && npm publish --tag latest"
  },
  "standard-version": {
    "scripts": {
      "prebump": "npm run check-git-status",
      "postbump": "npm run build-full && git add -u",
      "posttag": "git push --follow-tags"
    }
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.29",
    "@types/mocha": "^2.2.28",
    "@types/systemjs": "^0.18.27",
    "@types/three": "^0.84.20",
    "@types/webvr-api": "0.0.31",
    "browser-sync": "^2.18.13",
    "derequire": "^2.0.3",
    "http-server": "^0.10.0",
    "jspm": "0.17.0-beta.32",
    "node-static": "^0.7.7",
    "standard-version": "^4.2.0",
    "tsconfig-glob": "^0.2.1",
    "typedoc": "^0.5.7",
    "typescript": "^2.4.1",
    "typescript-formatter": "^2.3.0",
    "uglify-js": "^3.0.13"
  }
}
